{
  "name": "urdf-js",
  "version": "0.0.14",
  "description": "URDF Loader for THREE.js and webcomponent viewer",
  "main": "umd/URDFLoader.js",
  "scripts": {
    "start": "concurrently --kill-others \"rollup -c -w\" \"static-server\"",
    "build:rollup": "rollup -c",
    "build:webpack": "cd example && webpack --config ../webpack.config.js",
    "build": "npm run build:rollup && npm run build:webpack",
    "test": "rollup -c && jest",
    "lint": "eslint src/*.js ./test/*.js",
    "coverage": "nyc report --reporter=html & opn coverage/index.html"
  },
  "files": [
    "src/*",
    "umd/*",
    "example/index.js",
    "example/dragAndDrop.js",
    "example/styles.css"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapyuta-robotics/urdf-js.git"
  },
  "bugs": {
    "url": "https://github.com/rapyuta-robotics/urdf-js/issues"
  },
  "homepage": "https://github.com/rapyuta-robotics/urdf-js#readme",
  "keywords": [
    "javascript",
    "threejs",
    "graphics",
    "ros",
    "robotics",
    "urdf",
    "urdf-models",
    "webcomponents"
  ],
  "peerDependencies": {
    "three": ">=0.105.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.27.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nyc": "^15.1.0",
    "opn-cli": "^3.1.0",
    "puppeteer": "^22.9.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "rollup": "^2.52.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svg": "^2.0.0",
    "script-loader": "^0.7.2",
    "static-server": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "three": "^0.128.0",
    "threejs-model-loader": "0.0.1",
    "wc-loader": "^1.1.2",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "codemirror": "^6.0.1",
    "three-mesh-bvh": "^0.4.2",
    "xacro-parser": "^0.3.9"
  }
}
